{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://raw.githubusercontent.com/xbadiche/cumfidio/main/json_schema/datetime.schema.json",
    "title": "Date",
    "description": "Format YYYY-MM-DDThh:mm:ss",
    "required": [ "datetime" ],
    "type": "object",
    "properties": {
        "datetime" : {
            "type" : "string",
            "pattern" : "^\\d{4}-(((0[13578]|1[02])-(0[1-9]|[12][0-9]|3[01]))|((0[469]|11)-(0[1-9]|[12][0-9]|30))|(02-(0[1-9]|[12][0-9])))T(([01][0-9])|(2[0-3])):[0-5][0-9]:[0-5][0-9]$"
        }
    }
}